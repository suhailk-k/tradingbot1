# AI Optimization Settings
# Set to false to completely disable AI analysis
AI_ENABLED=true

# Maximum AI API calls per day (default: 50)
AI_DAILY_LIMIT=50

# Cache duration in minutes (default: 15)
AI_CACHE_DURATION_MINUTES=15

# Minimum signal strength to trigger AI analysis (0.0-1.0, default: 0.7)
AI_MINIMUM_SIGNAL_STRENGTH=0.7

# Signal confidence threshold for AI validation (default: 80)
AI_VALIDATION_THRESHOLD=80
